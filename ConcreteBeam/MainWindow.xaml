        <local:ConvertResult x:Key="ConvertDC"/>
        <Style TargetType="{x:Type ComboBox}">
            <Style.Triggers>
                <Trigger Property="IsEnabled"
                 Value="False">
                    <Setter Property="Foreground"
                    Value="Gray"/>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
